  "license": "MIT",
  "bugs": {
    "url": "https://github.com/n4ze3m/courier-otp/issues"
  },
  "homepage": "https://github.com/n4ze3m/courier-otp#readme",
  "devDependencies": {
    "@types/jest": "^29.1.2",
    "dotenv": "^16.0.3",
    "jest": "^29.1.2",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "tsc": "^2.0.4",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@trycourier/courier": "^3.15.0",
